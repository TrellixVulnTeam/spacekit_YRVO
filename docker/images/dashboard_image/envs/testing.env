CFG="dashboard"
APP="cal"
VERSION="3.2.0"
TAG="testing-${APP}-${CFG}-${VERSION}"
DOCKER_IMAGE="alphasentaurii/spacekit:${TAG}"
BASE_IMG="alphasentaurii/spacekit:base"
USEPYPI=1
SPACEKIT_REPO=""
SPACEKIT_BRANCH=""
SRC="pkg" # pkg, s3, git, file
COLLECTION="calcloud" # bucketname, repo url, or local path
DATASETS="2022-02-14,2021-11-04,2021-10-28"
DATES=('2022-02-14-1644848448' '2021-11-04-1636048291' '2021-10-28-1635457222')
PFX="archive" # for s3 this is the folder prefix
SPACEKIT_DATA=/home/developer
TF_CPP_MIN_LOG_LEVEL=2
CONTAINER_MODE="-d" # -d for detached, -it for interactive
MOUNTS="" # anything besides "" will bind mount the below source and dest paths
SOURCEDATA="$(pwd)/data"
DESTDATA="/home/developer/data"
HOSTNAME="localhost"
IPADDRESS=0.0.0.0
NAME="spkt-dash-${CFG}-${VERSION}"
EPCOMMAND="python -m spacekit.dashboard.${APP}.index"